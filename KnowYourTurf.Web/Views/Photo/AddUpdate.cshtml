@model PhotoViewModel
@using KnowYourTurf.Core.Html.FubuUI
@using KnowYourTurf.Web
@using KnowYourTurf.Web.Controllers
@{
    Layout = Model.Popup ? "~/Views/Shared/_popupLayout.cshtml" : "~/Views/Shared/_FormLayout.cshtml";
}

    <div class="col left">
        <div class="fieldRow">
            @Html.SubmissionFor(x => x.Name).ToHtmlTag()
        </div>
        <div class="fieldRow">
            @Html.SubmissionFor(x => x.Description).ToHtmlTag()
        </div>
        <div class="fieldRow">
            @Html.SubmissionFor(x => x.PhotoCategoryEntityId).LabelDisplay(WebLocalizationKeys.PHOTO_CATEGORY).ToHtmlTag()
        </div>
        <div class="fieldRow">
           @Html.SubmissionFor(x => x.FileUrl).ToHtmlTag() 
        </div>
    </div>

