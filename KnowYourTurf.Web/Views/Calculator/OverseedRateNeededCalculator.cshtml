@model KnowYourTurf.Web.Models.OverseedRateNeededCalcViewModel
@using KnowYourTurf.Core.Html.Expressions;
@using KnowYourTurf.Web
@using KnowYourTurf.Core.Html.FubuUI
@using KnowYourTurf.Web.Controllers;

@{
    Layout = "~/Views/Shared/_popupLayout.cshtml";
}

        <div id="puErrorMessages" class="puErrorMessages KYT_errorMessages" ><ul></ul></div>   
            @MvcHtmlString.Create(FormFor<CalculatorController>(x=>x.Calculate(null)).Class("CRUD").ElementId("CRUDForm").ToString())

            @MvcHtmlString.Create(Html.HiddenFor(x => x.EntityId).ToString())
            @MvcHtmlString.Create(Html.HiddenFor(x => x.CalculatorName).ToString())
            @MvcHtmlString.Create(Html.ViewFor(x => x.CalculatorDisplayName).ToString())
            @MvcHtmlString.Create(Html.SubmissionFor(x => x.Field).ToString())
            @MvcHtmlString.Create(Html.SubmissionFor(x => x.Product).ToString())
            @MvcHtmlString.Create(Html.SubmissionFor(x => x.BagsUsed).InputId("BagsUsed").ToString())
            @MvcHtmlString.Create(Html.SubmissionFor(x => x.OverSeedPercent).ToString())
            @MvcHtmlString.Create(Html.ViewFor(x => x.FieldArea).InputId("FieldArea").ToString())
            @MvcHtmlString.Create(Html.ViewFor(x => x.BagSize).InputId("BagSize").ToString())
            @MvcHtmlString.Create(Html.ViewFor(x => x.SeedRate).InputId("SeedRate").ToString())
            @MvcHtmlString.Create(EndForm())
